@prefix : <https://github.com/mbeclawski/cars-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://github.com/mbeclawski/cars-ontology> .

<https://github.com/mbeclawski/cars-ontology> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  https://github.com/mbeclawski/cars-ontology#hasBodyType
:hasBodyType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Car ;
             rdfs:range :CarBody .


###  https://github.com/mbeclawski/cars-ontology#hasBrand
:hasBrand rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Car ;
          rdfs:range :Brand .


###  https://github.com/mbeclawski/cars-ontology#hasEngine
:hasEngine rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Car ;
           rdfs:range :CarEngine .


#################################################################
#    Data properties
#################################################################

###  https://github.com/mbeclawski/cars-ontology#hasCityMPG
:hasCityMPG rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :hasMPG ;
            rdf:type owl:FunctionalProperty ;
            rdfs:range xsd:decimal .


###  https://github.com/mbeclawski/cars-ontology#hasDisplacement
:hasDisplacement rdf:type owl:DatatypeProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Combustion
                                             :Hybrid
                                           )
                             ] ;
                 rdfs:range xsd:double .


###  https://github.com/mbeclawski/cars-ontology#hasEngineDisplacement
:hasEngineDisplacement rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain :Car ;
                       rdfs:range xsd:decimal .


###  https://github.com/mbeclawski/cars-ontology#hasHighwayMPG
:hasHighwayMPG rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :hasMPG ;
               rdf:type owl:FunctionalProperty ;
               rdfs:range xsd:decimal .


###  https://github.com/mbeclawski/cars-ontology#hasMPG
:hasMPG rdf:type owl:DatatypeProperty ;
        rdfs:domain :Car .


###  https://github.com/mbeclawski/cars-ontology#hasModelYear
:hasModelYear rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Car ;
              rdfs:range xsd:integer .


###  https://github.com/mbeclawski/cars-ontology#hasNumberOfDoors
:hasNumberOfDoors rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :Car ;
                  rdfs:range xsd:integer .


###  https://github.com/mbeclawski/cars-ontology#hasNumberOfSeats
:hasNumberOfSeats rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :Car ;
                  rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  https://github.com/mbeclawski/cars-ontology#Battery
:Battery rdf:type owl:Class ;
         rdfs:subClassOf :CarEngine .


###  https://github.com/mbeclawski/cars-ontology#Brand
:Brand rdf:type owl:Class .


###  https://github.com/mbeclawski/cars-ontology#Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasNumberOfDoors ;
                       owl:someValuesFrom xsd:integer
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasNumberOfSeats ;
                       owl:someValuesFrom xsd:integer
                     ] .


###  https://github.com/mbeclawski/cars-ontology#CarBody
:CarBody rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Coupe
                                           :MiniVan
                                           :PickupTruck
                                           :SUV
                                           :Sedan
                                         )
                             ] .


###  https://github.com/mbeclawski/cars-ontology#CarEngine
:CarEngine rdf:type owl:Class .


###  https://github.com/mbeclawski/cars-ontology#Combustion
:Combustion rdf:type owl:Class ;
            rdfs:subClassOf :CarEngine .


###  https://github.com/mbeclawski/cars-ontology#Coupe
:Coupe rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Car
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasNumberOfDoors ;
                                                    owl:hasValue 2
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#Fast
:Fast rdf:type owl:Class ;
      rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#Hatchback
:Hatchback rdf:type owl:Class ;
           rdfs:subClassOf :Car ,
                           [ owl:intersectionOf ( :Car
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasNumberOfDoors ;
                                                    owl:hasValue 5
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] .


###  https://github.com/mbeclawski/cars-ontology#Hybrid
:Hybrid rdf:type owl:Class ;
        rdfs:subClassOf :CarEngine .


###  https://github.com/mbeclawski/cars-ontology#Hydrogen
:Hydrogen rdf:type owl:Class ;
          rdfs:subClassOf :CarEngine .


###  https://github.com/mbeclawski/cars-ontology#MPG
:MPG rdf:type owl:Class .


###  https://github.com/mbeclawski/cars-ontology#MiniVan
:MiniVan rdf:type owl:Class ;
         rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#Model
:Model rdf:type owl:Class .


###  https://github.com/mbeclawski/cars-ontology#PickupTruck
:PickupTruck rdf:type owl:Class ;
             rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#SUV
:SUV rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :Car
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasNumberOfDoors ;
                                                  owl:hasValue 5
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#Sedan
:Sedan rdf:type owl:Class ;
       rdfs:subClassOf [ owl:intersectionOf ( :Car
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasNumberOfDoors ;
                                                owl:hasValue 4
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] .


###  https://github.com/mbeclawski/cars-ontology#Sports
:Sports rdf:type owl:Class ;
        rdfs:subClassOf :Car .


###  https://github.com/mbeclawski/cars-ontology#10Cylinder
<https://github.com/mbeclawski/cars-ontology#10Cylinder> rdf:type owl:Class ;
                                                         rdfs:subClassOf :Combustion .


###  https://github.com/mbeclawski/cars-ontology#4Cylinder
<https://github.com/mbeclawski/cars-ontology#4Cylinder> rdf:type owl:Class ;
                                                        rdfs:subClassOf :Combustion .


###  https://github.com/mbeclawski/cars-ontology#6Cylinder
<https://github.com/mbeclawski/cars-ontology#6Cylinder> rdf:type owl:Class ;
                                                        rdfs:subClassOf :Combustion .


###  https://github.com/mbeclawski/cars-ontology#8Cylinder
<https://github.com/mbeclawski/cars-ontology#8Cylinder> rdf:type owl:Class ;
                                                        rdfs:subClassOf :Combustion .


